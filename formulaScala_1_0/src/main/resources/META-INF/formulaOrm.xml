<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns='http://java.sun.com/xml/ns/persistence/orm'
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:schemaLocation='http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/orm_1_0.xsd'
    version="1.0">

    <entity class="formula.Pcode" access="FIELD">
        <table name="pcode"/>
        <attributes>
            <id name="pcode"/>
            <basic name="obsolete"/>
            <basic name="series"/>
            <basic name="name"/></attributes></entity>
            
    <entity class="formula.Authorize" access="FIELD">
        <table name="authorize"/>
        <attributes>
            <id name="id"/>
            <basic name="person"/>
            <basic name="kana"/>
            <basic name="password"/></attributes></entity>
            
    <entity class="formula.Form2" access="FIELD">
        <table name="form2"/>
        <attributes>
            <id name="pcode"/>
            <basic name="date"/>
            <basic name="sg"/>
            <basic name="person"/>
            <basic name="comment"/>
            <basic name="reason"/>
            <basic name="confDate"/>
            <one-to-many name="form1" target-entity="formula.Form1" mapped-by="form2"/>
        </attributes></entity>

    <entity class="formula.Form1" access="FIELD">
        <table name="form1"/>
        <attributes>
            <embedded-id name="pk"/>
            <basic name="date"/>
            <basic name="mcode"/>
            <basic name="percent"/>
            <many-to-one name="form2" fetch="LAZY">
                <join-column name="pcode" insertable="false" updatable="false"/>
            </many-to-one></attributes></entity>

    <embeddable class="formula.Form1PK" access="FIELD">
        <attributes>
            <basic name="pcode"/>
            <basic name="order"/></attributes></embeddable>

</entity-mappings>